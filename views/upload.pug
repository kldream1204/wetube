extends layouts/main

block content
    .form-container
        form(action=`/videos${routes.upload}`, method='post')
            label(for='file') video File
            input(type="file", name='uploadVideo', id='file')
            input(type="text", name='title', placeholder='Title')
            textarea(name='description', placeholder='Deacription')
            input(type="submit", value='Up Load')